file "$(IP)/ipApp/Db/ADAM_4015.db"
{
pattern
{P,      R,     PORT,    A}
{IOC:,   adam1, rs485p1, 01}

}

# NOTE: Channel number must start from zero to match the protocol
file "$(IP)/ipApp/Db/ADAM_4015_chan.db"
{
pattern
{P,      R,     PORT,       A,  C} 
{IOC:,   adam1, rs485p1,   01,  0} 
{IOC:,   adam1, rs485p1,   01,  1} 
{IOC:,   adam1, rs485p1,   01,  2} 
{IOC:,   adam1, rs485p1,   01,  3} 
{IOC:,   adam1, rs485p1,   01,  4} 
{IOC:,   adam1, rs485p1,   01,  5} 

}

