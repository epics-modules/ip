# HP_Agilent PS 66xxA Series 

# Output records
record (ao, "$(P)$(R)setV") {
  field (DTYP, "stream")
  field (OUT, "@HP_Agilent_PS66xxA.proto setVoltLevel $(PORT) $(ADDR)")
  field (DRVL, "0")
  field (PREC, "3")
}

