record(ao, "$(P)$(C)write")
{
   field(DTYP, "stream")
   field(OUT,  "@JenaNV100D.proto writeValue $(PORT)")
   field(PREC, "3")
}

record(ai, "$(P)$(C)read")
{
   field(DTYP, "stream")
   field(INP,  "@JenaNV100D.proto readValue $(PORT)")
   field(PREC, "3")
   field(PINI, "YES")
}

record(bo, "$(P)$(C)cl")
{
   field(DTYP, "stream")
   field(OUT,  "@JenaNV100D.proto closedLoop $(PORT)")
   field(ZNAM, "Off")
   field(ONAM, "On")
}

record(ao, "$(P)$(C)kp")
{
   field(DTYP, "stream")
   field(OUT,  "@JenaNV100D.proto writeKp $(PORT)")
   field(PREC, "3")
}

record(ai, "$(P)$(C)kp_RBV")
{
   field(DTYP, "stream")
   field(INP,  "@JenaNV100D.proto readKp $(PORT)")
   field(PREC, "3")
   field(PINI, "YES")
   field(SCAN, "1 second")
}

