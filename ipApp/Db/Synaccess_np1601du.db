{
    field (VAL, 1)
    field (OUT, "$(P)$(R)userCalcOut1.DISA $(P)userCalcOut1:$(R)EnableCalc.RVAL")
    field (PINI, "YES")
}

record(calcout, "$(P)$(R)userCalcOut1")
{
    field (SCAN, "Passive")
    field (DISV, "0")
}

