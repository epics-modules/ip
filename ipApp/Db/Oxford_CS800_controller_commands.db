
record(ao, "$(P)$(R)RESTART") {
  field(DESC, "Start/Restart program")
  field(DTYP, "stream")
  field(OUT, "@Oxford_CS800.proto restart($(P)$(R)) $(CP)")
  field(SDIS, "$(P)$(R)DISABLE")
}

